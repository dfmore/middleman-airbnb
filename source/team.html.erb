<div id="team">
  <div class="team-content">
    <h1>This website was built with <i class="fas fa-heart"></i> by</h1>
    <ul class="list-inline">
      <% ["dfmore", "jamirene88"].each do |user| %>
        <li>
          <% link_to "https://kitt.lewagon.com/alumni/#{user}", target: "_blank" do %>
            <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user}", class: "img-circle" %>
          <% end %>
          <h2><%= user %></h2>
          <p>
            <% link_to "https://github.com/#{user}", target: "_blank" do %>
              <i class="fab fa-github"></i> <%= user %>
            <% end %>
          </p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
